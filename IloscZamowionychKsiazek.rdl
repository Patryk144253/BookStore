  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandText>select a.Imie as Imie , a.Nazwisko as Nazwisko, count(w.Walidacja) as IloscZamowionychKsiazek
from KsiegarniaSchemat.Spis_Autorow as a
join KsiegarniaSchemat.Spis_Zamowien as w on (a.IDAutora = w.IDAutora)
where w.Walidacja = '0' 
group by a.Imie,a.Nazwisko;</CommandText>
